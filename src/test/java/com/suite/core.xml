<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="test_core">
	<!-- TC1: genetics complete-->
	<test name="genetics complete">
		<classes>
			<class name="com.scripts.Genetics">
				<methods>
					<include name="genetics"></include>
				</methods>
			</class>
		</classes>
	</test>

	<!-- TC2: MG, PSS dashboard-->
	<test name="MG PSS Dashboard">
		<classes>
			<class name="com.scripts.Genetics">
				<methods>
					<include name="MG_PSS_dashboard"></include>
				</methods>
			</class>
		</classes>
	</test>

	<!-- TC3: genetics cancel-->
	<test name="genetics cancel">
		<classes>
			<class name="com.scripts.Genetics">
				<methods>
					<include name="cancel"></include>
				</methods>
			</class>
		</classes>
	</test>

	<!-- TC4: genetics denied-->
	<test name="genetics denied">
		<classes>
			<class name="com.scripts.Genetics">
				<methods>
					<include name="denied"></include>
				</methods>
			</class>
		</classes>
	</test>


	<!--TC5: MDL intake -->
	<test name="MDL intake">
		<classes>
			<class name="com.scripts.MDL_intake">
				<methods>
					<include name="MDL_intake"></include>
				</methods>
			</class>
		</classes>
	</test>



	<!--TC6: MDL patient -->
	<test name="MDL patient">
		<classes>
			<class name="com.scripts.MDL_patient">
				<methods>
					<include name="MDL_patient"></include>
				</methods>
			</class>
		</classes>
	</test>

	<!--TC7: weight loss -->
	<test name="weightloss">
		<classes>
			<class name="com.scripts.MDL_patient">
				<methods>
					<include name="weightloss"></include>
				</methods>
			</class>
		</classes>
	</test>

	<!--TC8: beauty -->
	<test name="beauty">
		<classes>
			<class name="com.scripts.beauty">
				<methods>
					<include name="beauty"></include>
				</methods>
			</class>
		</classes>
	</test>





</suite>