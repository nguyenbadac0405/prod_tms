<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="test_core">
	<!-- TC1: genetics complete-->
	<test name="genetics complete">
		<classes>
			<class name="com.scripts.Genetics">
				<methods>
					<include name="genetics"></include>
				</methods>
			</class>
		</classes>
	</test>


	<!-- TC3: genetics cancel-->
	<test name="genetics cancel">
		<classes>
			<class name="com.scripts.Genetics">
				<methods>
					<include name="cancel"></include>
				</methods>
			</class>
		</classes>
	</test>

	<!-- TC4: genetics denied-->
	<test name="genetics denied">
		<classes>
			<class name="com.scripts.Genetics">
				<methods>
					<include name="denied"></include>
				</methods>
			</class>
		</classes>
	</test>

	<!-- TC4: RPM-->
	<test name="RPM">
		<classes>
			<class name="com.scripts.RPM">
				<methods>
					<include name="RPM"></include>
				</methods>
			</class>
		</classes>
	</test>


</suite>