<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="test_core">
	<!--TC0: genetics complete -->
	<test name="genetics complete with wellness">
		<classes>
			<class name="com.scripts.Genetics">
				<methods>
					<include name="completeGeneticsWithWellness"></include>
				</methods>
			</class>
		</classes>
	</test>

	<!--TC1: genetics complete -->
	<test name="genetics complete without wellness">
		<classes>
			<class name="com.scripts.Genetics">
				<methods>
					<include name="completeGeneticNoWellness"></include>
				</methods>
			</class>
		</classes>
	</test>

    <!-- TC2: genetics cancel -->
	<test name="genetics cancel">
		<classes>
			<class name="com.scripts.Genetics">
				<methods>
					<include name="cancel"></include>
				</methods>
			</class>
		</classes>
	</test>

	<!-- TC3: genetics denied-->
	<test name="genetics denied">
		<classes>
			<class name="com.scripts.Genetics">
				<methods>
					<include name="denied"></include>
				</methods>
			</class>
		</classes>
	</test>

	<!-- : Delete case-->
	<test name="Delete case">
		<classes>
			<class name="com.scripts.Genetics">
				<methods>
					<include name="deleteCase"></include>
				</methods>
			</class>
		</classes>
	</test>

	<!-- TC4: RPM Smartmed complete-->
	<test name="RPM SmartMed complete">
		<classes>
			<class name="com.scripts.RPM">
				<methods>
					<include name="complete"></include>
				</methods>
			</class>
		</classes>
	</test>

	<!-- TC5: RPM SmartMed cancel-->
	<test name="RPM SmartMed cancel">
		<classes>
			<class name="com.scripts.RPM">
				<methods>
					<include name="cancel"></include>
				</methods>
			</class>
		</classes>
	</test>

	<!-- TC6: RPM SmartMed denied -->
	<test name="RPM SmartMed">
		<classes>
			<class name="com.scripts.RPM">
				<methods>
					<include name="denied"></include>
				</methods>
			</class>
		</classes>
	</test>
</suite>